<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Doctor" table="doctor">
    <id name="id" type="long" column="id">
    	<generator class="identify"></generator>
    </id>
    <property name="name" type="string" column="name"></property>
    <property name="gender" type="character" column="gender"></property>
    <property name="phoneNumber" type="string" column="phoneNumber"></property>
    <property name="address" type="string" column="address"></property>
    <property name="dateOfBirth" type="date" column="dateOfBirth"></property>
    <property name="departmentId" type="integer" column="departmentId"></property>
    </class>
</hibernate-mapping>